; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="godot_one_button_platformer"
config/features=PackedStringArray("4.3", "GL Compatibility")

[autoload]

PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"
SceneManager="*res://addons/scene_manager/SceneManager.tscn"
Shaker="*res://addons/shaker/src/Shaker.gd"
DebugMenu="*res://addons/debug_menu/debug_menu.tscn"

[editor_plugins]

enabled=PackedStringArray("res://addons/debug_menu/plugin.cfg", "res://addons/phantom_camera/plugin.cfg", "res://addons/scene_manager/plugin.cfg", "res://addons/shaker/plugin.cfg")

[layer_names]

2d_physics/layer_1="World"
2d_physics/layer_2="Player"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
